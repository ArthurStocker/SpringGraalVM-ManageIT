package org.manageit.core;

/**
 * 
 * 
 * 
 * 
 * 
 * 
 */
public interface IPropertyDescriptor extends IElement {

  /**
   * Set the Java Class to be used for this descriptor
   * 
   * @param javaclass as Class<?>
   */
  public void setNative(Class<?> javaclass);

  /**
   * Get the Java Class used for this descriptor
   * 
   * @return javaclass as Class<?>
   */
  public Class<?> getNative();
}