package org.manageit.core;

import java.util.UUID;

/**
 * 
 * 
 * 
 * 
 * 
 * 
 */
public interface IElement {

  /**
   * Set the UUID to identify the Element
   * 
   * @param __elixir_id as UUID
   */
  public void setId(UUID __elixir_id);

  /**
   * Get the UUID to identify the Element
   * 
   * @return __elixir_id as UUID
   */
  public UUID getId();

  /**
   * Set the Humanreadable name of the Element
   * 
   * @param __elixir_name as String
   */
  public void setName(String __elixir_name);

  /**
   * Get the Humanreadable name of the Element
   * 
   * @return __elixir_name String
   */
  public String getName();

  /**
   * Set the Type to be returned if the
   * Value of this Element is requested
   * 
   * @param __elixir_type as String
   */
  public void setType(String __elixir_type);
  
  /**
   * Get the Type to be returned if the
   * Value of this Element is requested
   * 
   * @return __elixir_type as String
   */
  public String getType();

  /**
   * Set the Label (DisplayName) name of the Element
   * 
   * @param __elixir_label as String
   */
  public void setLabel(String __elixir_label);

  /**
   * Get the Label (DisplayName) of the Element
   * 
   * @return __elixir_lable String
   */
  public String getLabel();
}